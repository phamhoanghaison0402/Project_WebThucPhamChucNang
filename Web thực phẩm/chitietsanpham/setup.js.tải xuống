/* leave this line in, in order to prevent configurator from regenerating it. namespace collision with client's own js. 
function initCarousel(element, totalItems, scrollItems, isVertical, fallbackDimension, callback)
*/

mybuys.luckyVitamin = {};
mybuys.luckyVitamin.addToCart = function(productId, quantity, cartType, callback)
{
	if (typeof AddToCartBySku == "function")
	{
		AddToCartBySku(("" + productId).trim(), "" + quantity, "" + cartType, callback);
	}
	return false;
};

mybuys.luckyVitamin.addReferenceProductToCart = function(recProductId, recQuantity, recCartType, callback)
{
	if (typeof jQuery == "undefined")
	{
		return false;
	}
	
	var productId = jQuery("input[name=hfProductID]").val();
	var variantId = jQuery("input[name=hfVariantID]").val();
	
	var cb = function()
	{
		mybuys.luckyVitamin.addToCart(recProductId, recQuantity, recCartType, callback);
	};
	
	if (typeof AddToCartScript == "function")
	{
		AddToCartScript(productId, variantId, 1, recCartType, 0, 0, null, cb);
	}
};

mybuys.luckyVitamin.removeFBTZone = function()
{
	jQuery("[mybuyszone=5]").remove();
	jQuery("[mybuyszone=21]").remove();
	jQuery("[mybuyszone=31]").remove();
};

mybuys.base_initPage = mybuys.initPage;
mybuys.initPage = function() 
{

//MAGNETIC BEACON HOME PAGE
if(mybuys.params["pt"] === "h") 
{
	var jsNode = document.createElement('script');
	jsNode.setAttribute('type', 'text/javascript');
	jsNode.setAttribute('src', '//magnetic.t.domdex.com/23422/pix.js?t=r&for=LUCKYVITAMIN_MyBuys');
	document.getElementsByTagName('body')[0].appendChild(jsNode);

}

//MAGNETIC BEACON ORDER CONF PAGE
else if (mybuys.params["pt"] === "purchase")
{

}
   
  //MAGNETIC BEACON CART PAGE
else if(mybuys.params["pt"] === "cart")
{
	var jsNode = document.createElement('script');
	jsNode.setAttribute('type', 'text/javascript');
	jsNode.setAttribute('src', '//magnetic.t.domdex.com/23425/pix.js?t=r&for=LUCKYVITAMIN_MyBuys');
	document.getElementsByTagName('body')[0].appendChild(jsNode);

   
}
   
   
  //MAGNETIC BEACON CATEGORY PAGE
else if(mybuys.params["pt"] === "cat" || mybuys.params["pt"] === "hcat")
{
	var jsNode = document.createElement('script');
	jsNode.setAttribute('type', 'text/javascript');
	jsNode.setAttribute('src', '//magnetic.t.domdex.com/23423/pix.js?t=r&for=LUCKYVITAMIN_MyBuys');
	document.getElementsByTagName('body')[0].appendChild(jsNode);

   
}   
  //MAGNETIC BEACON PRODUCT DETAILS PAGE
else if(mybuys.params["pt"] === "prod")
{
	var jsNode = document.createElement('script');
	jsNode.setAttribute('type', 'text/javascript');
	jsNode.setAttribute('src', '//magnetic.t.domdex.com/23424/pix.js?t=r&for=LUCKYVITAMIN_MyBuys');
	document.getElementsByTagName('body')[0].appendChild(jsNode);

   
}
//MAGNETIC BEACON OTHER PAGES
else {
	var jsNode = document.createElement('script');
	jsNode.setAttribute('type', 'text/javascript');
	jsNode.setAttribute('src', '//magnetic.t.domdex.com/23426/pix.js?t=r&for=LUCKYVITAMIN_MyBuys');
	document.getElementsByTagName('body')[0].appendChild(jsNode);}
	
	if((this.params["pt"]) && (this.params["pt"].indexOf("prod") > -1))
	{


		var productId = mybuys.params["productid"];
		mybuys.set("fbtProductId", productId);
		mybuys.set("fbtImageUrl", jQuery(".img-area img").first().attr("src"));
		
		// get the name and brand
		mybuys.set("fbtProductName", jQuery(".description-area h1, .description-area h1 span[itemprop=name]").last().html());
		mybuys.set("fbtProductBrand", jQuery("[itemprop=brand]").html());
		
		// get the price.
		var priceArea = jQuery(".price-area");
		
		//TEST!
		// Case 1: normal price stuff
		if (priceArea && priceArea.length > 0 && priceArea.find(".low").length == 0)
		{

			if (priceArea.find(".new-price .save").length == 0)
			{
				mybuys.set("fbtCurrentPrice", priceArea.find(".new-price, .new-price span").last().html().replace(/(?![,\.])([\.\D])+/g, "").replace(/,(?=\d\d\d)/g, "").replace(/(\d)\D(\d\d\d)/g, "$1$2").replace(/,/g, ".").trim());
			}
			else
			{
				var match = priceArea.find(".new-price").html().match(/\D+(\d*[\.,]\d*[\.]*\d*)/) || [];
				if (match.length > 1)
				{
					mybuys.set("fbtCurrentPrice", match[1].replace(/(?![,\.])([\.\D])+/g, "").replace(/\(.*?\)/g, "").replace(/,(?=\d\d\d)/g, "").replace(/(\d)\D(\d\d\d)/g, "$1$2").replace(/,/g, ".").trim());
				}
				else
				{
					mybuys.set("FBTremove1","true");
					mybuys.luckyVitamin.removeFBTZone();
				}
			}

			if (priceArea.find(".old-price").length > 0)
			{
				mybuys.set("fbtBasePrice", priceArea.find(".old-price, .old-price span").last().html().replace(/(?![,\.])([\.\D])+/g, "").replace(/MSRP:/g, "").replace(/(\d)\D(\d\d\d)/g, "$1$2").replace(/\(.*?\)/g, "").replace(/,(?=\d\d\d)/, "").replace(/,/g, ".").trim());
			}
			else 
			{
				mybuys.set("fbtBasePrice", mybuys.params["fbtcurrentprice"]);
			}

			mybuys.base_initPage();
		}
		
		// Case 2: "price too low to show"
		else
		{			
			// We need to make an ajax call to luckyvitamin to get the price
			jQuery.ajax("/previewCart?ProductID=" + productId,
				{
					success: function(data, status, thing)
					{
						var parser = new DOMParser();
						var doc = null;
						try
						{
							doc = parser.parseFromString(data, "text/html");
						}
						catch (e)
						{
							mybuys.set("FBTremove2","true");
							mybuys.luckyVitamin.removeFBTZone();
							mybuys.base_initPage();
							return;
						}

						if (jQuery(".price", doc).length > 0)
						{
							mybuys.set("fbtBasePrice", jQuery(".product-retail-price", doc).html().replace(/(?![,\.])([\.\D])+/g, "").replace(/(\d)\D(\d\d\d)/g, "$1$2").trim());
							mybuys.set("fbtCurrentPrice", jQuery(".product-sale-price", doc).html().replace(/(?![,\.])([\.\D])+/g, "").replace(/(\d)\D(\d\d\d)/g, "$1$2").trim());
						}
						else
						{
							mybuys.set("FBTremove3","true");
							mybuys.luckyVitamin.removeFBTZone();
						}

						mybuys.base_initPage();
					},

					complete: function(thing, status)
					{
						if (status != "notmodified" && status != "success")
						{
							mybuys.set("FBTremove","true");
							mybuys.luckyVitamin.removeFBTZone();
							mybuys.base_initPage();
						}
					}
				});
		}
	}
	else if((this.params["pt"]) && (this.params["pt"].indexOf("purchase") != -1))
     {
	 
		if (this.params["orderid"].indexOf("LV") == -1)
		{
				this.params["orderid"] = 'LV' + this.params["orderid"];

		} 
	 
        if(this.params['email'])
        {
             var testemail = Math.max(this.params['email'].toUpperCase().indexOf("@LUCKYVITAMIN.COM"),this.params['email'].toUpperCase().indexOf("QATEST@BRANDINGBRAND.COM"));
             if(testemail<0)
             {
                  mybuys.base_initPage();
             }
        }
        else
        {
             mybuys.base_initPage();
        }
     }
	else
	{
		mybuys.base_initPage();
	}
};



