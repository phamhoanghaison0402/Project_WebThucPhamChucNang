// NOTE: The default processResponseHTML implementation for wrz carousels has
// been modified to call through to the old implementation for jcarousellite
// carousels for this client.
mybuys.base_processResponseHTML = mybuys.processResponseHTML;
mybuys.processResponseHTML = function(zoneHtmls)
{
	this.base_processResponseHTML(zoneHtmls);

	// initialize our lightbox buttons
	if (mybuys.params["pt"] && mybuys.params["pt"] == "prod" && mybuys.params["wrz"] && (mybuys.params["wrz"].split(",").indexOf("5") > -1 || mybuys.params["wrz"].split(",").indexOf("21") > -1 || mybuys.params["wrz"].split(",").indexOf("31") > -1))
	{
		initLightbox();
	}
	
	var jQueryRequired = false;
	if (mbJQPageTypes.indexOf(mybuys.pagetype) > -1)
	{
		jQueryRequired = true;
	}
	if (jQueryRequired)
	{
		loadjQuery(
			function ()
			{
				if (typeof mbCarouselZoneKeys[mybuys.pagetype] != "undefined")
				{
					loadCarousel(
						function ()
						{
							var info = mbCarouselZoneKeys[mybuys.pagetype];
							for (var i = 0; i < info.length; i++)
							{
								if (mybuys.params.wrz.split(",").indexOf(info[i].zoneKey) > -1)
								{
									// Create a closure that first invokes the
									// callback, then loads any old zones.
									var callback = (function(x)
										{
											return function(carousel)
												{
													info[i].callback(carousel);
													mybuys.oldProcessResponseHTML(zoneHtmls);
												};
										})(i);
											
									mybuys.initCarousel('#mbcarousel' + info[i].zoneKey, info[i].totalItems, info[i].scrollItems, info[i].isVertical, info[i].fallbackDimension, callback); // Use our custom callback.
								}
							}
						}
					);
				}
				else
				{
					// Call through to load the old zones.
					mybuys.oldProcessResponseHTML(zoneHtmls);
				}
				
				if (mybuys.params["pt"] && mybuys.params["pt"] == "prod" && mybuys.params["wrz"] && (mybuys.params["wrz"].split(",").indexOf("5") > -1 || mybuys.params["wrz"].split(",").indexOf("21") > -1 || mybuys.params["wrz"].split(",").indexOf("31") > -1))
				{
					var popupBoxes = jQuery('.mb-popup-wrap');
					var resultItems = jQuery('.results-list > li');
					var toggleZ = function(item) {
						var boxes = item.find(popupBoxes);
						var state = !!boxes.filter('.popup-active').length;
						item.toggleClass('show-popup-state');
					}
					
					popupBoxes.contentPopup({
						mode: 'click',
						onShow: function(that) {
							toggleZ(that.holder.closest(resultItems));
						},
						onHide: function(that) {
							toggleZ(that.holder.closest(resultItems));
						}
					});
				}
			}
		);
	}
	else
	{
		// Call through to load the old zones.
		mybuys.oldProcessResponseHTML(zoneHtmls);
	}
};

mybuys.initCarousel = function(element, totalItems, scrollItems, isVertical, fallbackDimension, callback)
{
	jQuery(element).jcarousel(
		{
			wrap : 'circular',
			size : totalItems,
			scroll : scrollItems,
			vertical : isVertical,
			animation : 1000,
			initCallback : callback,
			buttonNextHTML : null,
			buttonPrevHTML : null,
			itemFallbackDimension : fallbackDimension
		}
	);
}

function mbcarouselInitCallback_cart3(carousel) 
{
	jQuery(".MB_CART3 .MB_CAROUSELLEFT").bind('click', function() { carousel.prev(); return false; });
	jQuery(".MB_CART3 .MB_CAROUSELRIGHT").bind('click', function() { carousel.next(); return false; });
	return false;
}

// NOTE: This function has been renamed and the logic for injecting zones has
// been removed in favor of calling the original processResponseHTML
// definition (stored in mybuys.base_procesResponseHTML), as defined by
// mybuys4.js
mybuys.oldProcessResponseHTML = function(zoneHtmls)
	{
	    if (mybuys.params['chn'] && mybuys.params['chn'] == 'm')
	    {
			jQuery(function() {
				jQuery(".mbCarousel").jCarouselLite({
					btnNext: ".mbCnext",
					btnPrev: ".mbCprev",
					visible: 2,
					scroll: 2,
					speed: 500
				});
			});
			jQuery(function() {
				jQuery(".mbCarousel2").jCarouselLite({
					btnNext: ".mbCnext2",
					btnPrev: ".mbCprev2",
					visible: 2,
					scroll: 2,
					speed: 500
				});
			});
		}
	};
	
