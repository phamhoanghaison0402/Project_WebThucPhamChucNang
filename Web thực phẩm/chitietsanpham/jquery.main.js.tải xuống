function initCustomHover(){jQuery('.cart-block').touchHover();jQuery('.user-account').touchHover();};(function($,window){$.fn.autoCompleteForm=function(opt){opt=$.extend({startCount:1,dataAttr:'q',ajaxAttr:'ajax=1',listItems:'li',listItemsFillsInput:true,alwaysRefresh:false,filterResults:true,highlightMatches:false,selectedClass:'selected-line',resultsHolder:'.ajax-holder',inputField:'input.text-input',hideDelay:200},opt);return this.each(function(){var form=$(this);var target=form.attr('action');var input=form.find(opt.inputField).attr('autocomplete','off');var ajaxHolder=form.find(opt.resultsHolder).hide();var acXHR,listItems,lastData,inFocus,focusTimer,visibleItems,visibleCount,currentIndex=0;if(opt.filterResults)opt.alwaysRefresh=false;function loadData(callback){if(acXHR&&typeof acXHR.abort==='function'){acXHR.abort();}
