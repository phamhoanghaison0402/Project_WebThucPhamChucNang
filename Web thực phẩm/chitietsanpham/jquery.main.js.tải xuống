function initSmoothScroll(){var panel=jQuery('#aside-nav , #jump-box');var links=panel.find('a');var page=jQuery('html,body');var animSpeed=500;var win=jQuery(window);var header=jQuery('#header');var minWidth=1300;links.each(function(){var link=jQuery(this);link.on('click',function(e){e.preventDefault();var currentTarget=link.attr('href');var offsetTop=currentTarget==='#'?0:jQuery(currentTarget).offset().top;page.animate({scrollTop:offsetTop},animSpeed);});});function panelPosition(){if(win.width()>minWidth){panel.show();}else{panel.hide();}}
