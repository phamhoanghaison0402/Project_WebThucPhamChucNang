return false;}}
function checkField(i,obj){var currentObject=jQuery(obj);var currentParent=currentObject.parents('div.row, .form-row');var currentPasswordParent=currentObject.parents('.password-valid');var firstPass=currentPasswordParent.find('#pass');var lastPass=currentPasswordParent.find('#pass2');if(currentObject.hasClass('required-password')){setState(currentPasswordParent,(firstPass,lastPass),!currentObject.val().length||currentObject.val()===currentObject.prop('defaultValue')||firstPass.val()!=lastPass.val());}
if(currentObject.hasClass('required')){setState(currentParent,currentObject,!currentObject.val().length||currentObject.val()===currentObject.prop('defaultValue'));}
if(currentObject.hasClass('required-email')){setState(currentParent,currentObject,!regEmail.test(currentObject.val()));}
if(currentObject.hasClass('required-number')){setState(currentParent,currentObject,!regPhone.test(currentObject.val()));}
if(currentObject.hasClass('required-select')){setState(currentParent,currentObject,currentObject.get(0).selectedIndex===0);}}
function setState(hold,field,error){hold.removeClass(errorClass).removeClass(successClass);if(error){hold.addClass(errorClass);field.one('focus',function(){hold.removeClass(errorClass).removeClass(successClass);});successFlag=false;}else{hold.addClass(successClass);}}
form.submit(validateForm);});}
function isIEoriPad(){try{var ua=window.navigator.userAgent;var msie=ua.indexOf('MSIE ');var ie11Plus=ua.indexOf('Trident/');var isiPad=ua.match(/iPad/i)!=null;return msie>0||ie11Plus>0||isiPad;}
catch(e){}
return false;}
