function initSlider(){jQuery('.slider-holder').each(function(){var holder=jQuery(this);var slider=holder.find('#slider-range');var min=parseInt(holder.find('.min-value').val());var max=parseInt(holder.find('.max-value').val());var from=holder.find('.from');var to=holder.find('.to');var fromTitle=holder.find('.get-from');var toTitle=holder.find('.get-to');var topRange=holder.find('.top-range');fromTitle.text(from.val());toTitle.text(to.val());slider.slider({range:true,min:min,max:max,values:[from.val(),to.val()],step:5,slide:function(event,ui){from.attr('value',ui.values[0]);to.attr('value',(isNaN(ui.values[1]))?to.val():ui.values[1]);fromTitle.text(ui.values[0]);toTitle.text((isNaN(ui.values[1]))?to.val():ui.values[1]);if(((isNaN(ui.values[1]))?to.val():ui.values[1]>topRange.attr('value'))&&!isNaN(topRange.attr('value'))){toTitle.text(topRange.attr('value')+' Over');}}})})}
